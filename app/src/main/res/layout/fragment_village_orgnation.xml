                        android:text="Yes" />

                    <RadioButton
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.6"
                        android:id="@+id/radiono"
                        android:text="No" />
                </RadioGroup>
            </LinearLayout>

            <LinearLayout
