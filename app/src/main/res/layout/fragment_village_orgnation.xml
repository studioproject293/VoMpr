                android:paddingLeft="10dp"
                android:weightSum="2">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="50dp"
                    android:layout_weight="0.7"
                    android:gravity="center|left"
                    android:text="Date"
                    android:textColor="@color/Black"
                    android:textSize="16sp" />

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="25dp"
                    android:layout_weight="1.3"
                    android:background="@drawable/dark_boder">

                    <TextView
                        android:id="@+id/date"
                        android:layout_width="match_parent"
                        android:textSize="16sp"
                        android:gravity="center"
                        android:textColor="@color/Black"
                        android:layout_height="50dp"
                        android:padding="10dp" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_margin="5dp"
                android:gravity="center"
                android:visibility="gone"
                android:orientation="horizontal"
                android:weightSum="2"
                android:paddingStart="10dp">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="50dp"
                    android:layout_weight="0.7"
                    android:gravity="center|left"
                    android:text="Day"
                    android:textColor="@color/Black"
                    android:textSize="16sp" />

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="25dp"
                    android:layout_weight="1.3"
                    android:background="@drawable/dark_boder">

                    <TextView
                        android:id="@+id/day"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:textSize="16sp"
                        android:gravity="center"
                        android:textColor="@color/Black"
                        android:padding="10dp" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_margin="5dp"
                android:gravity="center"
                android:orientation="horizontal"
                android:paddingLeft="10dp"
                android:weightSum="2">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="50dp"
                    android:layout_weight="0.7"
                    android:gravity="center|left"
                    android:text="VO Office type"
                    android:textColor="@color/Black"
                    android:textSize="16sp" />

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="25dp"
                    android:layout_weight="1.3"
                    android:background="@drawable/dark_boder">

                    <TextView
                        android:id="@+id/voOfficeType"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:textSize="16sp"
                        android:gravity="center"
                        android:textColor="@color/Black"
                        android:padding="10dp" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_margin="5dp"
                android:gravity="center"
                android:orientation="horizontal"
                android:paddingLeft="10dp"
                android:weightSum="2">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="50dp"
                    android:layout_weight="0.7"
                    android:gravity="center|left"
                    android:text="Address"
                    android:textColor="@color/Black"
                    android:textSize="16sp" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="100dp"
                    android:layout_marginLeft="25dp"
                    android:layout_weight="1.3"
                    android:textSize="16sp"
                    android:gravity="center"
                    android:id="@+id/text_address"
                    android:textColor="@color/Black"
                    android:background="@drawable/dark_boder"
                    android:backgroundTint="@null"
                    android:inputType="numberDecimal"
                    android:maxLines="5" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_margin="5dp"
                android:gravity="center"
                android:orientation="horizontal"
                android:paddingLeft="10dp"
                android:weightSum="2">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="50dp"
                    android:layout_weight="0.7"
                    android:gravity="center|left"
                    android:text="Contact Name"
                    android:textColor="@color/Black"
                    android:textSize="16sp" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="25dp"
                    android:layout_weight="1.3"
                    android:textSize="16sp"
                    android:gravity="center"
                    android:id="@+id/contactName"
                    android:textColor="@color/Black"
                    android:background="@drawable/dark_boder"
                    android:backgroundTint="@null"
                    android:inputType="numberDecimal" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_margin="5dp"
                android:gravity="center"
                android:orientation="horizontal"
                android:paddingLeft="10dp"
                android:weightSum="2">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="50dp"
                    android:layout_weight="0.7"
                    android:gravity="center|left"
                    android:text="Contact No."
                    android:textColor="@color/Black"
                    android:textSize="16sp" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="25dp"
                    android:layout_weight="1.3"
                    android:gravity="center"
                    android:textSize="16sp"
                    android:id="@+id/textViewContactNumber"
                    android:textColor="@color/Black"
                    android:background="@drawable/dark_boder"
                    android:backgroundTint="@null"
                    android:inputType="numberDecimal" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_margin="5dp"
                android:gravity="center"
                android:orientation="horizontal"
                android:paddingLeft="10dp"
                android:weightSum="2">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="50dp"
                    android:layout_weight="0.7"
                    android:gravity="center|left"
                    android:text="Total SHG in federation"
                    android:textColor="@color/Black"
                    android:textSize="16sp" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="25dp"
                    android:layout_weight="1.3"
                    android:gravity="center"
                    android:textColor="@color/Black"
                    android:id="@+id/totalscgmember"
                    android:background="@drawable/dark_boder"
                    android:backgroundTint="@null"
                    android:inputType="numberDecimal" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_margin="5dp"
                android:gravity="center"
                android:orientation="horizontal"
                android:paddingLeft="10dp"
                android:weightSum="2">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="50dp"
                    android:layout_weight="0.7"
                    android:gravity="center|left"
                    android:text="Total SHG in Village"
                    android:textColor="@color/Black"
                    android:textSize="16sp" />
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="25dp"
                    android:layout_weight="1.3"
                    android:gravity="center"
                    android:textColor="@color/Black"
                    android:id="@+id/textViewTotalSCGInVillage"
                    android:background="@drawable/dark_boder"
                    android:backgroundTint="@null"
                    android:inputType="numberDecimal" />
            </LinearLayout>

            <androidx.recyclerview.widget.RecyclerView
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

            </androidx.recyclerview.widget.RecyclerView>

        </LinearLayout>
    </ScrollView>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_alignParentBottom="true"
        android:orientation="horizontal"

        android:layout_height="55dp">
        <Button
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:layout_margin="5dp"
            android:background="@drawable/button_background"
            android:padding="10dp"
            android:id="@+id/addMember"
            android:textSize="16sp"
            android:textColor="#FFF"
            android:text="Add Member" />
        <Button
            android:layout_width="0dp"
            android:layout_weight="0.5"
            android:layout_height="55dp"
            android:layout_margin="5dp"
            android:background="@drawable/dark_boder"
            android:padding="10dp"
            android:visibility="gone"
            android:text="Save" />

    </LinearLayout>

</RelativeLayout>